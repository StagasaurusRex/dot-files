Host freenas
 HostName freenas.local
 User root
 AddKeysToAgent yes
 UseKeychain yes
 IdentityFile ~/.ssh/id_rsa

Host wol-prd
 HostName www02.prd.aws.workoutloud.com
 User ubuntu
 AddKeysToAgent yes
 UseKeychain yes
 IdentityFile ~/.ssh/wol-admin

Host wol-stg
 Hostname www02.stg.aws.workoutloud.com
 User ubuntu
 AddKeysToAgent yes
 UseKeychain yes
 IdentityFile ~/.ssh/wol-admin
 
Host *
 AddKeysToAgent yes
 UseKeychain yes
 IdentityFile ~/.ssh/id_rsa
